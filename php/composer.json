{
    "name": "yiisoft/app-api",
    "type": "project",
    "description": "Yii Framework API project template",
    "keywords": [
        "yii3",
        "golang",
        "api",
        "rest",
        "roadrunner"
    ],
    "homepage": "https://github.com/yiisoft/app-api/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/app-api/issues?state=open",
        "source": "https://github.com/yiisoft/app-api",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "chat": "https://t.me/yii3en"
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/yiisoft"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/yiisoft"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "aws/aws-sdk-php": "^3.336",
        "basis-company/nats": "^1.0",
        "cebe/markdown": "^1.2@dev",
        "doctrine/collections": "^2.2",
        "filp/whoops": "^2.16",
        "firebase/php-jwt": "^6.10",
        "google/protobuf": "^3.25",
        "grpc/grpc": "^1.57",
        "guzzlehttp/guzzle": "^7.9",
        "httpsoft/http-message": "^1.1",
        "khanamiryan/qrcode-detector-decoder": "^2.0",
        "nesbot/carbon": "^3.8",
        "psr/container": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.1|^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0",
        "ramsey/collection": "^2.0",
        "roadrunner-php/app-logger": "^1.2",
        "spiral/goridge": "^4.2",
        "spiral/roadrunner-grpc": "^3.4",
        "spiral/roadrunner-jobs": "^4.6",
        "spiral/roadrunner-services": "v2.2.0",
        "symfony/console": "^6.0",
        "symfony/event-dispatcher": "^7.1",
        "symfony/filesystem": "^7.2",
        "symfony/messenger": "^7.1",
        "symfony/process": "^7.2",
        "symfony/property-access": "^7.1",
        "symfony/scheduler": "^7.1",
        "symfony/string": "^7.1",
        "vlucas/phpdotenv": "^5.3",
        "w7corp/easywechat": "^6.15",
        "yiisoft/auth": "^3.1",
        "yiisoft/cache": "^3.0",
        "yiisoft/cache-file": "^3.0",
        "yiisoft/cache-redis": "^2.0",
        "yiisoft/config": "^1.0",
        "yiisoft/data": "^1.0",
        "yiisoft/data-response": "^2.0",
        "yiisoft/db-migration": "^1.1",
        "yiisoft/db-pgsql": "^1.3",
        "yiisoft/definitions": "^3.0",
        "yiisoft/di": "^1.2",
        "yiisoft/error-handler": "^3.0",
        "yiisoft/factory": "^1.1",
        "yiisoft/files": "^2.0",
        "yiisoft/http": "^1.2",
        "yiisoft/hydrator": "^1.0",
        "yiisoft/hydrator-validator": "^2.0",
        "yiisoft/input-http": "^1.0",
        "yiisoft/log": "^2.0",
        "yiisoft/log-target-file": "^3.0",
        "yiisoft/middleware-dispatcher": "^5.2",
        "yiisoft/mutex": "^1.1",
        "yiisoft/mutex-redis": "dev-dependabot/composer/predis/predis-tw-2.1",
        "yiisoft/request-body-parser": "^1.1",
        "yiisoft/router": "^3.0",
        "yiisoft/router-fastroute": "^3.0",
        "yiisoft/validator": "^1.0",
        "yiisoft/yii-console": "^2.0",
        "yiisoft/yii-debug": "dev-master",
        "yiisoft/yii-event": "^2.0",
        "yiisoft/yii-http": "^1.0",
        "yiisoft/yii-middleware": "^1.0",
        "yiisoft/yii-runner-console": "^2.0",
        "yiisoft/yii-runner-http": "^2.0",
        "ext-zip": "*",
        "yiisoft/yii-runner-roadrunner": "^3.0",
        "ext-pcntl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "autoload": {
        "psr-4": {
            "Common\\": "common"
        },
        "files": [
            "common/functions.php"
        ]
    },
    "scripts": {
        "format": "php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin-file": "configuration.php"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "yiisoft/config": true,
            "codeception/c3": true
        }
    }
}
